{
    "name"             : "espark-react",
    "version"          : "0.5.7",
    "author"           : {
        "name" : "Alan Tai"
    },
    "repository"       : {
        "type" : "git",
        "url"  : "https://github.com/ayltai/espark.git"
    },
    "bugs"             : {
        "url" : "https://github.com/ayltai/espark/issues"
    },
    "keywords"         : [
        "esp32",
        "iot"
    ],
    "license"          : "MIT",
    "module"           : "dist/index.es.js",
    "main"             : "dist/index.cjs.js",
    "types"            : "dist/src/index.d.ts",
    "type"             : "module",
    "files"            : [
        "dist"
    ],
    "scripts"          : {
        "clean"   : "pnpm dlx rimraf --glob **/node_modules/* pnpm-lock.yaml",
        "reset"   : "pnpm clean && pnpm i && pnpm audit -P",
        "lint"    : "eslint src",
        "pretest" : "pnpm lint",
        "test"    : "cross-env VITE_COVERAGE=true vitest --run",
        "test:ci" : "cross-env NODE_OPTIONS=--max-old-space-size=1536 VITE_COVERAGE=true vitest --run",
        "build"   : "vite build && tsc -p tsconfig.build.json"
    },
    "peerDependencies" : {
        "react"     : "^19.2.3",
        "react-dom" : "^19.2.3"
    },
    "dependencies"     : {
        "@ant-design/icons"                 : "^6.1.0",
        "@fortawesome/fontawesome-svg-core" : "^7.1.0",
        "@fortawesome/free-solid-svg-icons" : "^7.1.0",
        "@fortawesome/react-fontawesome"    : "^3.1.1",
        "@refinedev/antd"                   : "^6.0.3",
        "@refinedev/core"                   : "^5.0.8",
        "@refinedev/react-router"           : "^2.0.3",
        "@tanstack/react-query"             : "^5.90.16",
        "antd"                              : "^6.1.4",
        "date-fns"                          : "^4.1.0",
        "i18next"                           : "^25.7.3",
        "react-i18next"                     : "^16.5.1",
        "react-redux"                       : "^9.2.0",
        "react-router"                      : "^7.11.0",
        "string-to-color"                   : "^2.2.2"
    },
    "devDependencies"  : {
        "@eslint/eslintrc"           : "^3.3.3",
        "@eslint/js"                 : "^9.39.2",
        "@refinedev/cli"             : "^2.16.50",
        "@stylistic/eslint-plugin"   : "^5.6.1",
        "@testing-library/jest-dom"  : "^6.9.1",
        "@testing-library/react"     : "^16.3.1",
        "@types/node"                : "^25.0.3",
        "@types/react"               : "^19.2.7",
        "@types/react-dom"           : "^19.2.3",
        "@types/react-test-renderer" : "^19.1.0",
        "@typescript-eslint/parser"  : "^8.52.0",
        "@vitejs/plugin-react-swc"   : "^4.2.2",
        "@vitest/coverage-v8"        : "^4.0.16",
        "@vitest/ui"                 : "^4.0.16",
        "cross-env"                  : "^10.1.0",
        "eslint"                     : "^9.39.2",
        "eslint-plugin-import"       : "^2.32.0",
        "eslint-plugin-prefer-arrow" : "^1.2.3",
        "eslint-plugin-react"        : "^7.37.5",
        "eslint-plugin-react-hooks"  : "^7.0.1",
        "eslint-plugin-unicorn"      : "^62.0.0",
        "globals"                    : "^17.0.0",
        "jsdom"                      : "^27.4.0",
        "prop-types"                 : "^15.8.1",
        "typescript"                 : "^5.9.3",
        "typescript-eslint"          : "^8.52.0",
        "vite"                       : "^7.3.0",
        "vitest"                     : "^4.0.16",
        "vitest-fetch-mock"          : "^0.4.5"
    },
    "resolutions"      : {
        "qs" : "^6.14.1"
    },
    "engines"          : {
        "node" : ">=22",
        "pnpm" : ">=10"
    },
    "publishConfig"    : {
        "access" : "public"
    }
}
